<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lckiss.intentcase">
    <!--申请电话权限-->
    <uses-permission android:name="android.permission.CALL_PHONE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".IntentOne.SecondActivity">
            <intent-filter>
                <action android:name="Soft" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".ThridActivity">
            <intent-filter>
                <action android:name="Soft" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="Third" />
            </intent-filter>
        </activity>
        <activity android:name=".IntentOne.FourthActivity">

            <intent-filter>
                <action android:name="Soft1" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

    </application>

</manifest>